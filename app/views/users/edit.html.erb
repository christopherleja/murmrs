<%= form_for @user do |f| %>
<%= f.label :username %>
<h2>Edit your account</h2>
<%= f.text_field :username %>
<br>
<%= f.label :email %>
<%= f.text_field :email %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.label :bio %>
<%= f.text_area :bio %>
<br>
<%= f.label :avatar %>
<%= f.string :avatar %>
<br>
<%= f.submit%>
<%end%>